{
  "name": "dory",
  "version": "0.1.2",
  "description": "Markdown blogging platform with automatic publishing to Surge.sh, using advanced JavaScript/HTML5 techniques for a much loved UX.",
  "scripts": {
    "test": "ava tests/**/*.js && stylelint public/css/*.css public/css/**/*.css public/css/**/**/*.css && eslint public/js/**/*.js",
    "catalogue": "babel core/catalogue.js | node && mkdir -p core/build/assets/posts && cp public/posts/* core/build/assets/posts",
    "build": "babel core/cli/build.js | node",
    "start": "babel server/default.js | node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Dory.git"
  },
  "keywords": [
    "blog",
    "blogging",
    "writing",
    "write",
    "universal",
    "isomorphic"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Dory/issues"
  },
  "homepage": "https://github.com/Wildhoney/Dory#readme",
  "dependencies": {
    "array.from": "^1.0.0",
    "axios": "^0.9.1",
    "es6-promise": "^3.0.2",
    "keo": "^2.1.1",
    "lodash": "^4.2.1",
    "moment": "^2.11.2",
    "object-hash": "^1.1.2",
    "pluralize": "^1.2.1",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "sort-by": "^1.1.1",
    "yaml-front-matter": "^3.4.0"
  },
