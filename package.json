{
  "name": "dory",
  "version": "0.1.0",
  "description": "Markdown blogging platform with automatic publishing to Surge.sh, using advanced JavaScript/HTML5 techniques for a much loved UX.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "catalogue": "babel core/catalogue.js | node",
    "build": "webpack && npm run catalogue && npm run minify-html && npm run minify-js && npm run favicon",
    "dev": "webpack-dev-server --content-base core/build",
    "start": "babel server/default.js | node",
    "minify-js": "uglifyjs --compress --mangle --source-map core/build/dory.map -- core/build/dory.js > core/build/dory.min.js && npm run finalise-build",
    "minify-html": "html-minifier public/index.html --collapse-whitespace > core/build/index.html",
    "finalise-build": "rimraf core/build/dory.js && mv core/build/dory.min.js core/build/dory.js",
    "favicon": "cp public/favicon.ico core/build/favicon.ico"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Dory.git"
  },
  "keywords": [
    "blog",
    "blogging",
    "writing",
    "write",
    "universal",
    "isomorphic"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Dory/issues"
  },
  "homepage": "https://github.com/Wildhoney/Dory#readme",
  "dependencies": {
    "array.from": "^1.0.0",
    "babel-cli": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "es6-promise": "^3.0.2",
    "keo": "^1.5.2",
    "lodash": "^4.2.1",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "mustache": "^2.2.1",
    "node-sass": "^3.4.2",
    "redux": "^3.3.1",
    "webpack": "^2.0.7-beta",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "html-minifier": "^1.1.1",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "uglify-js": "^2.6.1",
    "webpack": "^2.0.7-beta",
    "webpack-dev-server": "^1.14.1",
    "yaml-front-matter": "^3.3.0"
  }
}
