{
  "name": "dory",
  "version": "0.1.0",
  "description": "Markdown blogging platform with automatic publishing to Surge.sh, using advanced JavaScript/HTML5 techniques for a much loved UX.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "babel core/publish.js | node",
    "generate": "babel core/generate.js | node",
    "build": "npm run process-html",
    "dev": "node-sass public/sass/default.scss public/.assets/default.css",
    "process-html": "htmlprocessor public/index.html -o core/build/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Dory.git"
  },
  "keywords": [
    "blog",
    "blogging",
    "writing",
    "write"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Dory/issues"
  },
  "homepage": "https://github.com/Wildhoney/Dory#readme",
  "dependencies": {
    "array.from": "^1.0.0",
    "babel-cli": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "es6-promise": "^3.0.2",
    "js-yaml": "^3.5.2",
    "keo": "^1.5.2",
    "lodash": "^4.2.1",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "mustache": "^2.2.1",
    "node-sass": "^3.4.2",
    "redux": "^3.3.1",
    "webpack": "^2.0.6-beta",
    "whatwg-fetch": "^0.11.0",
    "xml-parse-from-string": "^1.0.0",
    "xmldom": "^0.1.22"
  }
}
