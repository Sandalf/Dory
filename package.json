{
  "name": "dory",
  "version": "0.1.1",
  "description": "Markdown blogging platform with automatic publishing to Surge.sh, using advanced JavaScript/HTML5 techniques for a much loved UX.",
  "scripts": {
    "test": "stylelint public/sass/*.scss public/sass/**/*.scss public/sass/**/**/*.scss && eslint public/js/**/*.js",
    "catalogue": "babel core/catalogue.js | node",
    "build": "babel core/cli/build.js | node",
    "config": "yaml2json dory.yml > core/build/assets/config.json",
    "dev": "webpack-dev-server --content-base core/build",
    "start": "babel server/default.js | node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Dory.git"
  },
  "keywords": [
    "blog",
    "blogging",
    "writing",
    "write",
    "universal",
    "isomorphic"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Dory/issues"
  },
  "homepage": "https://github.com/Wildhoney/Dory#readme",
  "dependencies": {
    "array.from": "^1.0.0",
    "axios": "^0.9.1",
    "es6-promise": "^3.0.2",
    "keo": "^2.1.1",
    "lodash": "^4.2.1",
    "moment": "^2.11.2",
    "object-hash": "^1.1.2",
    "pluralize": "^1.2.1",
    "react-router": "^2.0.0",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.5.2",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.4",
    "html-minifier": "^1.1.1",
    "js-yaml": "^3.5.3",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "mustache": "^2.2.1",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "shelljs": "^0.6.0",
    "stylelint": "^4.4.0",
    "stylelint-config-standard": "^3.0.0",
    "uglify-js": "^2.6.1",
    "webpack": "^2.0.7-beta",
    "webpack-dev-server": "^1.14.1",
    "yaml-front-matter": "^3.3.0",
    "yamljs": "^0.2.6"
  }
}
