  "bugs": {
    "url": "https://github.com/Wildhoney/Dory/issues"
  },
  "homepage": "https://github.com/Wildhoney/Dory#readme",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "js-yaml": "^3.5.2",
    "lodash": "^4.2.1",
    "mustache": "^2.2.1",
    "node-sass": "^3.4.2"
