    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.5.2",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^3.0.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
